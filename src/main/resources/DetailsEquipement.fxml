<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="com.esprit.controllers.EquipementController">
    <children>
        <Label layoutX="20" layoutY="20" text="Nom Équipement" />
        <TextField fx:id="tfNomEquipement" layoutX="150" layoutY="20" />

        <Label layoutX="20" layoutY="60" text="Quantité" />
        <TextField fx:id="tfQuantite" layoutX="150" layoutY="60" />

        <Label layoutX="20" layoutY="100" text="ID Espace" />
        <TextField fx:id="tfIdEspace" layoutX="150" layoutY="100" />

        <Button layoutX="150" layoutY="140" onAction="#updateEquipement" text="Mettre à jour" />
        <Button layoutX="250" layoutY="140" onAction="#deleteEquipement" text="Supprimer" />
    </children>
</AnchorPane>
